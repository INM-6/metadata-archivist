{
    "$schema": "https://abc",
    "$id": "https://abc.json",
    "description": "my example schema",
    "type": "object",
    "properties": {
        "metadata_archive": {
            "type": "object",
            "properties": {
                "program_execution": {
                    "type": "object",
                    "properties": {
                        "time_info": {
                            "$ref": "#/$defs/time_parser"
                        },
                        "model_configuration": {
                            "$ref": "#/$defs/yml_parser"
                        }
                    }
                }
            }
        }
    },
    "$defs": {
        "time_parser": {
            "type": "object",
            "properties": {
                "real": {
                    "type": "string",
                    "description": "the time from start to finish of the call"
                },
                "user": {
                    "type": "string",
                    "description": "amount of CPU time spent in user mode"
                },
                "sys": {
                    "type": "string",
                    "description": "amount of CPU time spent in kernel mode"
                },
                "system": {
                    "$ref": "#/properties/sys"
                }
            }
        },
        "yml_parser": {},
        "other_block": {
            "type": "object",
            "properties": {
                "nested_info": {
                    "type": "object",
                    "properties": {
                        "user_time_info": {
                            "$ref": "#/$defs/time_parser/user"
                        }
                    }
                }
            }
        }
    }
}