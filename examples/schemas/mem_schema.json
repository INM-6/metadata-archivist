{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "description": "Memory info during runtime derived using: cat /proc/meminfo \n descriptions taken from: https://www.thegeekdiary.com/understanding-proc-meminfo-file-analyzing-memory-utilization-in-linux/",
    "type": "object",
    "properties": {
        "MemTotal": {
            "type": "string",
            "description": "Total usable ram (i.e. physical ram minus a few reserved bits and the kernel binary code)"
        },
        "MemFree":{
            "type": "string",
            "description": "Is sum of LowFree+HighFree (overall stat)"
        },
        "MemAvailable":{
            "type": "string"
        },
        "Buffers":{
            "type": "string",
            "description": "Memory in buffer cache. mostly useless as metric nowadays Relatively temporary storage for raw disk blocks shouldn’t get tremendously large (20MB or so)"
        },
        "Cached":{
            "type": "string",
            "description": "Memory in the pagecache (diskcache) minus SwapCache, Doesn’t include SwapCached"
        },
        "SwapCached":{
            "type": "string",
            "description": "Memory that once was swapped out, is swapped back in but still also is in the swapfile (if memory is needed it doesn’t need to be swapped out AGAIN because it is already in the swapfile. This saves I/O )"
        },
        "Active":{
            "type": "string",
            "description": "Memory that has been used more recently and usually not reclaimed unless absolute necessary."
        },
        "Inactive":{
            "type": "string"
        },
        "Active(anon)":{
            "type": "string"
        },
        "Inactive(anon)":{
            "type": "string"
        },
        "Active(file)":{
            "type": "string"
        },
        "Inactive(file)":{
            "type": "string"
        },
        "Unevictable":{
            "type": "string"
        },
        "Mlocked":{
            "type": "string"
        },
        "SwapTotal":{
            "type": "string"
        },
        "SwapFree":{
            "type": "string"
        },
        "Dirty":{
            "type": "string"
        },
        "Writeback":{
            "type": "string"
        },
        "AnonPages":{
            "type": "string"
        },
        "Mapped":{
            "type": "string"
        },
        "Shmem":{
            "type": "string"
        },
        "KReclaimable":{
            "type": "string"
        },
        "Slab":{
            "type": "string"
        },
        "SReclaimable":{
            "type": "string"
        },
        "SUnreclaim":{
            "type": "string"
        },
        "KernelStack":{
            "type": "string"
        },
        "PageTables":{
            "type": "string"
        },
        "NFS_Unstable":{
            "type": "string"
        },
        "Bounce":{
            "type": "string"
        },
        "WritebackTmp":{
            "type": "string"
        },
        "CommitLimit":{
            "type": "string"
        },
        "Committed_AS":{
            "type": "string"
        },
        "VmallocTotal":{
            "type": "string"
        },
        "VmallocUsed":{
            "type": "string"
        },
        "VmallocChunk":{
            "type": "string"
        },
        "Percpu":{
            "type": "string"
        },
        "HardwareCorrupted":{
            "type": "string"
        },
        "AnonHugePages":{
            "type": "string"
        },
        "ShmemHugePages":{
            "type": "string"
        },
        "ShmemPmdMapped":{
            "type": "string"
        },
        "FileHugePages":{
            "type": "string"
        },
        "FilePmdMapped":{
            "type": "string"
        },
        "HugePages_Total":{
            "type": "string"
        },
        "HugePages_Free":{
            "type": "string"
        },
        "HugePages_Rsvd":{
            "type": "string"
        },
        "HugePages_Surp":{
            "type": "string"
        },
        "Hugepagesize":{
            "type": "string"
        },
        "Hugetlb":{
            "type": "string"
        },
        "DirectMap4k":{
            "type": "string"
        },
        "DirectMap2M":{
            "type": "string"
        },
        "DirectMap1G":{
            "type": "string"
        }    },
    "unevaluatedProperties": false
}


